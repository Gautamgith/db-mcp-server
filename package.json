{
  "name": "postgresql-mcp-server",
  "version": "1.0.0",
  "description": "HTTP-based MCP server for PostgreSQL with IAM authentication and OAuth security",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "node --test dist/test/**/*.js",
    "test:connection": "tsx src/test/connection.ts",
    "test:iam": "tsx src/test/iam-connection.ts"
  },
  "keywords": ["mcp", "postgresql", "iam", "aws", "rds", "oauth", "http", "entraid"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "pg": "^8.12.0",
    "@aws-sdk/client-rds": "^3.600.0",
    "@aws-sdk/rds-signer": "^3.600.0",
    "@aws-sdk/credential-providers": "^3.600.0",
    "express": "^4.18.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.1.0",
    "@azure/identity": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.0",
    "@types/express": "^4.17.0",
    "@types/jsonwebtoken": "^9.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.0",
    "eslint": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0"
  }
}